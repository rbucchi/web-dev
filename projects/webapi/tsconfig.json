{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "strict": true,
        "allowJs": false,
        "outDir": "dist",
        "sourceMap": true,
        "noImplicitAny": false,
        "strictPropertyInitialization": false,
        "strictNullChecks": false
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "**/node_modules",
        "**/build",
        "**/dist",
        "**/*.md",
    ]
}